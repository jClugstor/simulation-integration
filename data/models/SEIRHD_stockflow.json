        "target": "flow2"
      },
      {
        "id": "link7",
        "source": "I",
        "target": "flow2"
      },
      {
        "id": "link8",
        "source": "cdelta",
        "target": "flow2"
      },
      {
        "id": "link9",
        "source": "I",
        "target": "flow3"
      },
      {
        "id": "link10",
        "source": "H",
        "target": "flow3"
      },
      {
        "id": "link11",
        "source": "cgamma",
        "target": "flow3"
      },
      {
        "id": "link12",
        "source": "hosp",
        "target": "flow3"
      },
      {
        "id": "link13",
        "source": "I",
        "target": "flow4"
      },
      {
        "id": "link14",
        "source": "R",
        "target": "flow4"
      },
      {
        "id": "link15",
        "source": "cgamma",
        "target": "flow4"
      },
      {
        "id": "link16",
