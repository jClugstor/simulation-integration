      }
    ]
  },
  "semantics": {
    "ode": {
        "parameters": [
        {
            "id": "p_cbeta",
            "name": "p_cbeta",
            "description": "transmission rate",
            "value": 0.35,
            "distribution": {
              "type": "Uniform1",
              "parameters":{
                "minimum": 0.25,
                "maximum": 0.45
              }
            }
        },
        {
            "id": "p_N",
            "name": "p_N",
            "description": "overall population",
            "value": 1001
        },
        {
            "id": "p_cdelta",
            "name": "p_cdelta",
            "description": "latency period",
            "value": 0.2,
            "distribution": {
              "type": "Uniform1",
              "parameters":{
                "minimum": 0.1,
                "maximum": 0.3
              }
            }
        },
        {
            "id": "p_cgamma",
